{
  "name": "ag-grid-angular",
  "version": "24.1.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "npx gulp copy-from-module-source && ng build --prod && npm run verify-properties",
    "build-cjs": "npm run build",
    "test": "ng test --watch false --browsers ChromeHeadless",
    "lint": "ng lint",
    "update-properties": "node -e \"require('./updateGridAndColumnProperties.js').updatePropertiesBuilt()\"",
